<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cookos.Controllers.ClientTicketMenuController">
   <children>
      <TextField fx:id="questionText" alignment="TOP_LEFT" layoutX="196.0" layoutY="98.0" prefHeight="155.0" prefWidth="206.0" promptText="Enter your question" />
      <Text layoutX="209.0" layoutY="47.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Write a ticket">
         <font>
            <Font size="31.0" />
         </font>
      </Text>
      <Button fx:id="backButton" layoutX="11.0" layoutY="358.0" mnemonicParsing="false" onAction="#onBackButtonClick" text="Back">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button fx:id="submitButton" layoutX="254.0" layoutY="283.0" mnemonicParsing="false" onAction="#onSubmitClickButton" text="Submit">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
