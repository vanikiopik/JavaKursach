<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cookos.Controllers.DeliveryController">
   <children>
      <ComboBox fx:id="deliveryCityComboBox" layoutX="224.0" layoutY="117.0" onAction="#onSelectedValue" prefWidth="150.0" promptText="City" />
      <Text layoutX="146.0" layoutY="71.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Delivery calculator">
         <font>
            <Font size="38.0" />
         </font>
      </Text>
      <Button fx:id="backButton" layoutX="14.0" layoutY="345.0" mnemonicParsing="false" onAction="#onBackButtonClick" text="Back">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <Text layoutX="93.0" layoutY="240.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Your delivery will be:">
         <font>
            <Font size="21.0" />
         </font>
      </Text>
      <Text fx:id="delieveryPrice" layoutX="300.0" layoutY="240.0" strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="121.0">
         <font>
            <Font size="19.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
