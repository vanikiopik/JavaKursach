<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cookos.AdminReviewOrderController">
   <children>
      <ComboBox fx:id="chooseIdComboBox" layoutX="14.0" layoutY="39.0" onAction="#onSelectedIdValue" prefWidth="150.0" promptText="Choose Id" />
      <Text layoutX="15.0" layoutY="129.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Product name:">
         <font>
            <Font size="20.0" />
         </font>
      </Text>
      <TextField fx:id="productNameField" editable="false" layoutX="157.0" layoutY="109.0" prefHeight="26.0" prefWidth="124.0" promptText="Choose ID" />
      <Text layoutX="14.0" layoutY="167.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Product type:">
         <font>
            <Font size="20.0" />
         </font>
      </Text>
      <TextField fx:id="productTypeField" editable="false" layoutX="157.0" layoutY="147.0" prefHeight="26.0" prefWidth="124.0" promptText="Choose ID" />
      <Text layoutX="17.0" layoutY="207.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Price:">
         <font>
            <Font size="20.0" />
         </font>
      </Text>
      <TextField fx:id="productPriceField" editable="false" layoutX="157.0" layoutY="187.0" prefHeight="26.0" prefWidth="124.0" promptText="Choose ID" />
      <Text layoutX="17.0" layoutY="244.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Delivery:">
         <font>
            <Font size="20.0" />
         </font>
      </Text>
      <TextField fx:id="productDeliveryField" editable="false" layoutX="157.0" layoutY="224.0" prefHeight="26.0" prefWidth="124.0" promptText="Choose ID" />
      <Text layoutX="17.0" layoutY="282.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Review status:">
         <font>
            <Font size="20.0" />
         </font>
      </Text>
      <ComboBox fx:id="reviewStatusComboBox" layoutX="157.0" layoutY="262.0" onAction="#onSelectedStatusValue" prefHeight="26.0" prefWidth="124.0" promptText="Select status" />
      <Button fx:id="confirmButton" layoutX="504.0" layoutY="342.0" mnemonicParsing="false" onAction="#onConfirmButtonClick" text="Confirm">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="backButton" layoutX="11.0" layoutY="342.0" mnemonicParsing="false" onAction="#onBackButtonClick" text="Back">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
